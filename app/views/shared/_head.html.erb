<title><%= Rails.configuration.application_name %></title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<%= favicon_link_tag 'favicon.png', type: 'image/png' %>

<%= csrf_meta_tags %>
<%= csp_meta_tag %>

<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'main', 'data-turbolinks-track': 'reload' %>

<script src="https://kit.fontawesome.com/0dfb90484a.js" data-search-pseudo-elements defer crossorigin="anonymous"></script>

<%= yield :head %>
